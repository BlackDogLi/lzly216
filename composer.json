{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^7.3|^8.0",
        "appstract/laravel-opcache": "^2.0",
        "erusev/parsedown": "^1.6",
        "fideloper/proxy": "^4.0",
        "graham-campbell/exceptions": "^15.0",
        "intervention/image": "^2.4",
        "laravel/framework": "^8.0",
        "laravel/helpers": "^1.4",
        "laravel/passport": "^10.0",
        "laravel/tinker": "^2.0",
	"guzzlehttp/guzzle":"^7.3",
	"nunomaduro/collision":"^5.0",
        "league/html-to-markdown": "^4.6",
        "naux/auto-correct": "^1.0",
        "orangehill/iseed": "^2.5",
        "overtrue/laravel-lang": "^3.0",
        "predis/predis": "^1.1.1",
        "suin/php-rss-writer": "^1.5",
        "laravel/ui": "^3.3",
        "facade/ignition": "^2.13"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.2",
        "barryvdh/laravel-ide-helper": "^2.5",
        "filp/whoops": "^2.0",
        "fakerphp/faker": "^1.12",
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^9.3",
        "symfony/css-selector": "^5.0",
        "symfony/dom-crawler": "^5.0"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/",
		  	"Models\\": "models/",
		  	"Middleware\\": "Http/middleware",
		  	"Controllers\\": "Http/controller",
		    "Factorymethod\\": "app/Factorymethod",
            "ThirdService\\": "app/Thirdservice"
        },
	  	"files": ["app/Helper/function.php"]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
	  	"post-install-cmd": [
		  	"Illuminate\\Foundation\\ComposerScripts::postInstall"
		],
	  	"post-update-cmd": [
		  	"Illuminate\\Foundation\\ComposerScripts::postUpdate",
		  	"php artisan ide-helper:generate",
		  	"php artisan ide-helper:meta"
		],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist"
    },
    "respositories": {
	 "packagist": {
	     "type": "composer",
             "url": "https://packagist.phpcomposer.com"
	}
    }
}
